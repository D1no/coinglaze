{
  "name": "coinglaze",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "apollo-cache-inmemory": "^1.4.2",
    "apollo-client": "^2.4.12",
    "apollo-link": "^1.2.6",
    "apollo-link-rest": "^0.7.0",
    "apollo-link-retry": "^2.2.8",
    "graphql": "^14.1.1",
    "graphql-anywhere": "^4.1.27",
    "graphql-tag": "^2.10.1",
    "p-throttle": "^2.1.0",
    "react": "^16.7.0",
    "react-apollo": "^2.4.0",
    "react-dom": "^16.7.0",
    "react-scripts": "2.1.3",
    "rebass": "^3.0.1",
    "styled-components": "^4.1.3",
    "styled-system": "^3.2.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "cypress run",
    "eject": "react-scripts eject",
    "cypress:open": "cypress open",
    "format": "prettier --list-different --write \"*/**/*.{ts,tsx,js,jsx,json,html,css}\"",
    "precommit": "lint-staged",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "prettier"
    ]
  },
  "prettier": {
    "singleQuote": false,
    "jsxBracketSameLine": true,
    "trailingComma": "es5",
    "endOfLine": "lf"
  },
  "lint-staged": {
    "{src,cypress}/**/*.{ts,tsx,js,jsx,json,html,css}": [
      "prettier --list-different --write",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/preset-react": "^7.0.0",
    "@cypress/webpack-preprocessor": "^4.0.3",
    "@storybook/addon-actions": "^4.1.11",
    "@storybook/addon-knobs": "^4.1.11",
    "@storybook/addon-links": "^4.1.11",
    "@storybook/addon-viewport": "^4.1.11",
    "@storybook/addons": "^4.1.11",
    "@storybook/react": "^4.1.11",
    "babel-loader": "8.0.4",
    "cypress": "^3.1.4",
    "cypress-react-unit-test": "^1.2.1",
    "eslint-config-prettier": "^4.0.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "prettier": "^1.16.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
